<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="view_partner_form_inherit_library_extension" model="ir.ui.view">
            <field name="name">res.partner.view.form.inherit.library.extension</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='category_id']" position="after">
                    <field name="my_book_ids" />
                </xpath>
            </field>
        </record>
    
    </data>
    

</odoo>
